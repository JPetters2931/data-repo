<% if f.object.class.multiple? key %>
  <%= f.input key, as: :multi_value_with_help, input_html: { class: 'form-control datarepo-datepicker' }, required: f.object.required?(key) %>
<% else %>
  <%= f.input key, input_html: { class: 'datarepo-datepicker' }, required: f.object.required?(key) %>
<% end %>
